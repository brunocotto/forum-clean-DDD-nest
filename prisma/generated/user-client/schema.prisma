generator userClient {
  provider     = "prisma-client-js"
  output       = "./generated/user-client"
  relationMode = "prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("USER_DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  name     String
  password String
  email    String @unique
  phone    String
  company  String

  @@map("users")
}
